  "accessibility": {
    "focusRing": {
      "color": "#6366F1",
      "width": "2px",
      "offset": "2px"
    },
    "contrast": {
      "minimum": "4.5:1",
      "preferred": "7:1"
    },
    "motion": {
      "respectReducedMotion": true,
      "fallbackToNoAnimation": true
    }
  },
  "designPrinciples": {
    "cleanMinimalism": {
