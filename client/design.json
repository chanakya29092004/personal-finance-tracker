    "dataFirst": {
      "description": "Design prioritizes data visibility and quick insights",
      "elements": ["Clear metrics", "Intuitive charts", "Scannable tables"]
    },
    "professionalTrust": {
      "description": "Builds confidence through consistent, reliable interface patterns",
      "features": ["Consistent spacing", "Reliable interactions", "Clear feedback"]
    },
    "developerFocused": {
      "description": "Tailored specifically for developer workflows and needs",
