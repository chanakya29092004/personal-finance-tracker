  "responsiveLayout": {
    "mobile": {
      "sidebar": "hidden",
      "navigation": "bottom-tab",
      "padding": "16px",
      "statsGrid": "grid-cols-1"
    },
    "tablet": {
      "sidebar": "collapsible",
